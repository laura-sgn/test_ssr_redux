{
    "presets": [
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic",
                    "importSource": "@emotion/react"
                }
            }
        ]
    ],
    "plugins": [
        "@emotion/babel-plugin",
        [
            "babel-plugin-root-import",
            {
                "rootPathSuffix": "./src",
                "rootPathPrefix": "~/"
            }
        ]
    ]
}
